container_commands:
  01_installnode:
    command: "yum install -y nodejs"
  02_runnodev:
    command: "npm install"
  03_runnodev:
    command: "npm run build"
  04_initkey:
    command: "php artisan migrate:fresh --seed"
  05_clear1:
    command: "php artisan cache:clear"
  06_clear2:
    command: "php artisan config:clear"
  07_clear3:
    command: "php artisan view:clear"
